   "source": [
    "n = int(input(\"enter a number to check if it is a prime or not:\"))\n",
    "count = 0\n",
    "for i in range(1, n+1) :\n",
    "    if not (n % i) : count += 1\n",
    "if (n == 0) or (n == 1) or (count >=3) : print(n, \"is not a prime number.\")\n",
    "else : print(n, \"is a prime number\")"
   ]
  }
